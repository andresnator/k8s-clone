{
    "clusters": [
        {
            "name": "source"
        },
        {
            "name": "dest"
        }
    ],
    "namespaces": {
        "source": [
            {
                "name": "source"
            }
        ],
        "dest": [
            {
                "name": "dest"
            }
        ]
    },
    "services": {
        "source": [
            {
                "name": "backend"
            },
            {
                "name": "frontend"
            }
        ]
    },
    "deployments": {
        "source": [
            {
                "name": "backend-deployment"
            },
            {
                "name": "frontend-deployment"
            }
        ]
    },
    "configMaps": {
        "source": [
            {
                "name": "app-config"
            }
        ]
    },
    "secrets": {
        "source": [
            {
                "name": "db-credentials"
            },
            {
                "name": "tls-cert"
            }
        ]
    },
    "persistentVolumeClaims": {
        "source": [
            {
                "name": "source-data-pvc"
            }
        ]
    },
    "apps": [
        {
            "name": "Example-APP",
            "context": "source",
            "namespaces": "source",
            "services": [
                {
                    "resource": "backend"
                },
                {
                    "resource": "frontend"
                }
            ],
            "deployments": [
                {
                    "resource": "backend-deployment",
                    "overwrite-spec": {
                        "replicas": 3
                    }
                },
                {
                    "resource": "frontend-deployment"
                }
            ],
            "configMaps": [
                {
                    "resource": "app-config"
                }
            ],
            "secrets": [
                {
                    "resource": "db-credentials"
                },
                {
                    "resource": "tls-cert"
                }
            ],
            "persistentVolumeClaims": [
                {
                    "resource": "source-data-pvc",
                    "overwrite-spec": {
                        "capacity": {
                            "storage": "2Gi"
                        }
                    }
                }
            ]
        }
    ]
}